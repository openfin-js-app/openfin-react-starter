  "scripts": {
    "start": "node scripts/start.js",
    "start-openfin": "node scripts/start.openfin.js",
    "build": "rimraf build && rimraf package && node scripts/build.js",
    "serve": "node scripts/server.js",
    "serve-openfin": "node scripts/server.openfin.js",
    "package": "node scripts/package.js",
    "test": "node scripts/test.js"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,mjs}",
      "!src/**/*.d.ts",
      "!src/serviceWorker.js",
      "!src/index.tsx"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{ts,tsx,js,jsx,mjs}",
      "**/?(*.)(spec|test).{ts,tsx,js,jsx,mjs}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {},
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(ts|tsx|js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "pkg": {
    "scripts": [
      "scripts/**/*.js",
      "config/**/*.js"
    ],
    "assets": [
      "build/**/*",
      "package.json"
    ]
  }
}
