{
  "defaultSeverity": "warn",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions":{
    "exclude":[
      "node_modules/**",
      "build/**"
    ]
  },
  "jsRules": {
    "curly":false,
    "no-console":false
  },
  "rules": {
    "curly": false,
    "no-console": false,
    "member-access": false
  },
  "rulesDirectory": []
}